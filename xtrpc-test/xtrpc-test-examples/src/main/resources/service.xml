<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:xt="https://github.com/lahahana/schema/xtrpc"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            https://github.com/lahahana/schema/xtrpc https://github.com/lahahana/schema/xtrpc/xt.xsd">
    <xt:registry address="http://localhost:8080" />
    <xt:application name="mockService1" />
    <xt:protocol name="xt" />
    <xt:service interface="com.github.lahahana.xtrpc.test.service.AddressService" ref="addressService"/>
    <bean id="addressService" class="com.github.lahahana.xtrpc.test.service.impl.AddressServiceImpl"></bean>
</beans>